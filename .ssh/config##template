Host github github.com
	HostName github.com
	IdentityFile ~/.ssh/github
	User git

Host gitlab.com
	IdentityFile ~/.ssh/e-seikatsu-gitlab

Host kmc.gr.jp
	User mikan
	IdentityFile ~/.ssh/kmc

# KMCの各マシンに <マシン名>.box2.kmc.gr.jp で入れるようにするための設定
Host *.box2.kmc.gr.jp
	ProxyJump kmc.gr.jp
	IdentityFile ~/.ssh/kmc
	User     mikan

# 主要なマシンに <マシン名> のみで入れるようにするための設定
# ringo以外にも、Host ringo tachyon sister...のように列挙可能
Host ringo   
	HostName %h.box2.kmc.gr.jp
	ProxyJump kmc.gr.jp
	User mikan
	IdentityFile ~/.ssh/kmc

Host kagoya
	HostName 133.18.236.168
	User ansai
	IdentityFile ~/.ssh/vps

Host cherry
	ProxyCommand cloudflared access ssh --hostname cherry.piny940.com
	IdentityFile ~/.ssh/cherry
	Port 53823
	User ansai

