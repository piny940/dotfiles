Host tk*
	HostName 10.229.71.249

Host ua*
	HostName 10.229.71.219

Host sq
	HostName sq.inet.media.kyoto-u.ac.jp

Host *-sq
	ProxyJump sq

Host *
	IdentityFile ~/.ssh/ed25519
	User ansai
